generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // Change this if you're using a different database
  url      = env("DATABASE_URL") // This will read from your .env file
}

// Example model
model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
}
